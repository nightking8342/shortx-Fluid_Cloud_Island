{
    "name": "阿里云盘",
    "type": "url",
    "tigger": [
      "alipan\\.com"
    ],
    "check": "com.alicloud.databox",
    "Custom_variable": [
      {
        "name": "shareid",
        "pattern": "\\/[st]\\/(.*)",
        "text": "link",
        "index": 1
      }
    ],
    "rule": [
      {
        "condition": [
          {
            "condition_type": "withcode"
          },
            {
            "condition_type": "contains",
            "condition_text": "link",
            "condition_pattern": "\\/s\\/"
          },{
            "condition_type": "notcontains",
            "condition_text": "link",
            "condition_pattern": "\\/t\\/"
          }
        ],
        "many_condition_tactic": "All",
        "rule_text": "smartdrive://share/browse?shareId=【shareid】&sharePwd=【code】"
      },
        {
        "condition": [
          {
            "condition_type": "withcode"
          },
            {
            "condition_type": "notcontains",
            "condition_text": "link",
            "condition_pattern": "\\/s\\/"
          },{
            "condition_type": "contains",
            "condition_text": "link",
            "condition_pattern": "\\/t\\/"
          }
        ],
        "many_condition_tactic": "All",
        "rule_text": "smartdrive://friendshare/browse?shareId=【shareid】&sharePwd=【code】"
      },
      {
          "condition": [
          {
